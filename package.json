    "check-perms": "node scripts/checkPermissions.js",
    "backup": "node scripts/backup.js",
    "validate-manifests": "node scripts/validateManifests.js",
    "generate:sdk": "npx openapi-typescript-codegen --input openapi.yaml --output sdks/ts --client fetch --useOptions --useUnionTypes",
